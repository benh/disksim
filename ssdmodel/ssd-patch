@@ -1364,14 +1387,44 @@
 lp_search_path(char *cwd, char *name)
 {
   char *cand = calloc(LP_PATH_MAX, sizeof(char));
+#ifdef _WIN32
+  struct _stat s;
+#else
   struct stat s;
+#endif
   int i;
   
+#ifndef _WIN32
   if(name[0] == '/')
     if(stat(name, &s))
       goto fail;
+    else
+      goto succ;
 
   snprintf(cand, LP_PATH_MAX, "%s/%s", cwd, name);
+  
+#else
+  if(name[0] == '\\')
+    if(stat(name, &s))
+      goto fail;
+    else
+      goto succ;
+
+  if (strcmp(cwd, "") == 0)
+      cwd = ".";
+  
+  snprintf(cand, LP_PATH_MAX, "%s\\%s", cwd, name);
+
+  /* use \ separator instead of / */
+    {
+        char *sep = cand;
+        while (*sep)
+        {
+            if (*sep == '/') *sep = '\\';
+            sep++;
+        }
+    }
+#endif
 
   if(!stat(cand, &s))
     goto succ;
